   "source": [
    "from scipy.integrate import quad\n",
    "from scipy.special import airy\n",
    "from matplotlib import pyplot as plt\n",
    "from numpy import linspace\n",
    "\n",
    "N=100000\n",
    "\n",
    "def f(x):\n",
    "    return airy(x)[0]\n",
    "\n",
    "x=linspace(-100,10,N+1)\n",
    "y=f(x)\n",
    "%matplotlib qt\n",
    "plt.plot(x,y)"
   ]
